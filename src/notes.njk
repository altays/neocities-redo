{% pageHeading  "Notes", "Here's a thing!"%}

<ul class="blog-list">
    {% for post in collections.post %}
        <li class="blog-list-item">
            <a href="{{ post.url }}" class="blog-list-link">{{ post.data.title }}</a>
            <small class="blog-list-date">{{post.date | postDate}}</small>
            <p class="blog-list-date">{{post.data.description}}</p>
        </li>
    {% endfor %}
</ul>