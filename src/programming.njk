{% pageHeading  "Programming", "Selected projects in various states of completions."%}

<section role>
    <h3>Collaborations</h3>
    <ul role="list">
    {% for project in programs.collaborations %}
    <li class="card">
        <article>
            <h4>
                <a href="{{project.link}}" title="Go to Github" aria-label="Go to Github (opens up in a new tab)" target="_blank">{{project.title}}</a>
            </h4>
            <p>
                {{project.description}}
            </p>
        </article>
    </li>
    {% endfor %}
    </ul>
</section>

<section>
    <h3>Tools</h3>
    <ul role="list">
    {% for project in programs.tools %}
    <li class="card">
        <article>
            <h4>
                <a href="{{project.link}}" title="Go to Bandcamp" aria-label="Go to Bandcamp (opens up in a new tab)" target="_blank">{{project.title}}</a>
            </h4>
            <p>
                {{project.description}}
            </p>
        </article>
    </li>
    {% endfor %}
    </ul>
</section>